{
  "name": "summer-2022-team",
  "version": "1.0.0",
  "description": "mvn-internship",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nodemon --legacy-watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "typeorm migration:create ./src/database/migrations/$npm_config_name",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/shared/configs/data-source.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/shared/configs/data-source.config.ts",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ./src/shared/configs/seed.config.ts"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "firebase": "^9.9.3",
    "firebase-admin": "^11.0.1",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "i18n": "^0.15.0",
    "joi": "^17.6.0",
    "minio": "^7.0.32",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.1",
    "typeorm": "^0.3.4",
    "typeorm-seeding": "^1.6.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@faker-js/faker": "^7.3.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/http-errors": "^1.8.2",
    "@types/i18n": "^0.13.4",
    "@types/minio": "^7.0.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "10.7.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.5.2"
  }
}
